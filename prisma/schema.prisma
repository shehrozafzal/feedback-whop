generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  id           String   @id @default(cuid())
  experienceId String
  userId       String
  content      String
  sentiment    String?  // optional (positive, neutral, negative)
  visible      Boolean  @default(false)
  createdAt    DateTime @default(now())
}